{
  "name": "update-service-worker",
  "version": "0.2.7",
  "description": "Simple CLI for updating the angular ngsw-config.json with current app version from package.json for PWAs.",
  "main": "./lib/index.js",
  "bin": {
    "update-service-worker": "./lib/index.js"
  },
  "keywords": [
    "angular",
    "service worker",
    "ngsw",
    "version",
    "package"
  ],
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec ts-node src/index.ts -p package.json -n ngsw-config.json",
    "create": "npm run build && npm run test",
    "build": "tsc -p .",
    "local": "sudo npm i -g && pizza",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
    "repopublish": "npm version patch && npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mareklesko/update-service-worker.git"
  },
  "homepage": "https://github.com/mareklesko/update-service-worker",
  "author": "Marek Lesko",
  "license": "ISC",
  "dependencies": {
    "commander": "^6.1.0",
    "jsonfile": "^6.0.1"
  },
  "devDependencies": {
    "@types/node": "^14.10.3",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
}
